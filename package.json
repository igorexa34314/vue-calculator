{
	"name": "vue-calculator",
	"description": "Simple calculator on Vue 3 (Quasar UI)",
	"version": "1.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build && cp dist/index.html dist/404.html",
		"build:analyze": "export ROLLUP_ANALYZE=true || set ROLLUP_ANALYZE=true && vue-tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write --cache src/"
	},
	"dependencies": {
		"@quasar/extras": "^1.16.7",
		"@vueuse/core": "^10.5.0",
		"mathjs": "^12.0.0",
		"pinia": "^2.1.7",
		"quasar": "^2.13.0",
		"vue": "^3.3.8",
		"vue-router": "^4.2.5"
	},
	"devDependencies": {
		"@quasar/vite-plugin": "^1.6.0",
		"@rushstack/eslint-patch": "^1.5.1",
		"@types/node": "^20.8.10",
		"@vitejs/plugin-vue": "^4.4.0",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/tsconfig": "^0.4.0",
		"eslint": "^8.53.0",
		"eslint-plugin-vue": "^9.18.1",
		"prettier": "^3.0.3",
		"rollup-plugin-visualizer": "^5.9.2",
		"sass": "1.32.12",
		"typescript": "^5.2.2",
		"unplugin-vue-router": "^0.7.0",
		"vite": "^4.5.0",
		"vue-tsc": "^1.8.22"
	},
	"packageManager": "pnpm@8.7.0"
}
